{
  "name": "@aah/service-user",
  "version": "2.0.0",
  "private": true,
  "description": "User management microservice for Athletic Academics Hub",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "@aah/database": "workspace:*",
    "@aah/auth": "workspace:*",
    "@aah/api-utils": "workspace:*",
    "@aah/config": "workspace:*",
    "@clerk/backend": "^1.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "vitest": "^2.1.0",
    "@vitest/coverage-v8": "^2.1.0"
  }
}
