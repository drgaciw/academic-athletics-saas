{
  "name": "@aah/service-integration",
  "version": "2.0.0",
  "private": true,
  "description": "Integration service for external systems (LMS, SIS, email, calendar)",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm,cjs --external isomorphic-fetch",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aah/api-utils": "workspace:*",
    "@aah/auth": "workspace:*",
    "@aah/config": "workspace:*",
    "@aah/database": "workspace:*",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@vercel/blob": "^0.22.0",
    "canvas-api": "^1.0.0",
    "googleapis": "^133.0.0",
    "hono": "^4.0.0",
    "pdfkit": "^0.14.0",
    "resend": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/pdfkit": "^0.13.0",
    "jest": "^29.7.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "integration",
    "lms",
    "sis",
    "email",
    "calendar"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}
