{
  "name": "@aah/ai-evals",
  "version": "1.0.0",
  "description": "AI evaluation framework for Athletic Academics Hub",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "echo 'AI Evals CLI - Use: npm run eval, npm run compare, or npm run report'",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "eval": "tsx cli.ts eval",
    "compare": "tsx cli.ts compare",
    "report": "tsx cli.ts report"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "@ai-sdk/anthropic": "^0.0.51",
    "ai": "^3.4.9",
    "zod": "^3.23.8",
    "@aah/ai": "workspace:*",
    "@aah/database": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "table": "^6.8.2",
    "@vercel/analytics": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "typescript": "^5.6.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.13"
  },
  "keywords": [
    "ai",
    "evaluation",
    "testing",
    "llm",
    "quality-assurance"
  ]
}
